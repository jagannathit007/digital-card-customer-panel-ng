{
  "name": "itfuturz-admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:godigital && npm run build:digitalcard",
    "build:godigital": "ng build --configuration=godigital && powershell Compress-Archive -Path dist/* -DestinationPath godigital.zip -Force",
    "build:digitalcard": "ng build --configuration=production && powershell Compress-Archive -Path dist/* -DestinationPath digitalcard.zip -Force",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
